# GenLayer Configuration
# Copy this file to .env.local and fill in your values

# Network: 'studio' for local development, 'testnet' for GenLayer testnet
VITE_NETWORK=studio

# Contract address - get this after deploying the contract
VITE_CONTRACT_ADDRESS=

# GenLayer Studio URL (for local development)
VITE_GENLAYER_STUDIO_URL=http://localhost:4000/api

# GenLayer Testnet URL
VITE_GENLAYER_TESTNET_URL=https://testnet.genlayer.com/api
